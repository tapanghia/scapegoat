<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
     <class name="com.marklabs.researchProject.ResearchProjectMetadata" table="researchproject_metadata">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
		<property name="projectName"/>
		<property name="design" type="integer"/>
		<property name="fragrance" type="integer"/>
		<property name="persistence" type="integer"/>
		<property name="packaging" type="integer"/>
		<property name="safety" type="integer"/>
		
		<property name="teamName" />
		<property name="scenario" type="integer"/>
		<property name="requestedBaseCost" type="integer"/>
		
	
     </class>
</hibernate-mapping>


